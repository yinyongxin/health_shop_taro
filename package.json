{
  "name": "taro-vite-tailwindcss-v4",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "None",
    "framework": "React"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5 -- --watch",
    "build:h5:local": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:harmony-hybrid": "taro build --type harmony-hybrid",
    "dev:weapp": "pnpm run build:weapp -- --watch",
    "dev:swan": "pnpm run build:swan -- --watch",
    "dev:alipay": "pnpm run build:alipay -- --watch",
    "dev:tt": "pnpm run build:tt -- --watch",
    "dev:h5": "pnpm run build:h5:local -- --watch",
    "dev:rn": "pnpm run build:rn -- --watch",
    "dev:qq": "pnpm run build:qq -- --watch",
    "dev:jd": "pnpm run build:jd -- --watch",
    "dev:harmony-hybrid": "pnpm run build:harmony-hybrid -- --watch",
    "open": "weapp open -p",
    "postinstall": "weapp-tw patch",
    "openapi-ts:dev": "cross-env NODE_ENV=development openapi-ts",
    "openapi-ts:prod": "cross-env NODE_ENV=production openapi-ts"
  },
  "browserslist": {
    "development": [
      "defaults and fully supports es6-module",
      "maintained node versions"
    ],
    "production": [
      "last 3 versions",
      "Android >= 4.1",
      "ios >= 8"
    ]
  },
